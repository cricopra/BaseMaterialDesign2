<android.support.v7.widget.CardView
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cvActivosFijosItem"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    card_view:cardCornerRadius="3dp"
    android:layout_marginTop="3dp"
    android:padding="0dp"
    android:clickable="true"
    android:foreground="?android:selectableItemBackground">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtIdActivoFijoLabelCvActivoFijoItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lbl_IdActivosFijos"
                android:textColor="@android:color/black"
                android:textSize="20dp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/txtIdActivoFijoCvActivoFijoItem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="IdActivo"
                android:textColor="@android:color/black"
                android:textSize="18dp"
                android:scrollHorizontally="true"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_below="@+id/txtIdActivoFijoLabelCvActivoFijoItem" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtNombreLabelCvActivoFijoItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lbl_NombreActivosFijos"
                android:textColor="@android:color/black"
                android:textSize="18dp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/txtNombreCvActivoFijoItem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Nombre"
                android:textColor="@android:color/black"
                android:textSize="16dp"
                android:scrollHorizontally="true"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_below="@+id/txtNombreLabelCvActivoFijoItem" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtUbicacionLabelCvActivoFijoItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lbl_UbicacionActivosFijos"
                android:textColor="@android:color/black"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_below="@+id/txtNombreCvActivoFijoItem"
                />
            <TextView
                android:id="@+id/txtUbicacionCvActivoFijoItem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="IdActivo"
                android:textColor="@android:color/black"
                android:textSize="16dp"
                android:scrollHorizontally="true"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_below="@+id/txtUbicacionLabelCvActivoFijoItem" />

        </LinearLayout>

    </LinearLayout>

